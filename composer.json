{
    "name": "beytullahk54/settings-package",
    "description": "Laravel için ayarlar yönetim paketi - Dinamik ayarları veritabanında saklama ve yönetme",
    "type": "library",
    "version": "1.0.0",
    "keywords": ["laravel", "settings", "configuration", "database"],
    "license": "MIT",
    "authors": [
        {
            "name": "Beytullah Kahraman",
            "email": "byt.kahraman@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3|8.4",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Beytullahk54\\SettingsPackage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Beytullahk54\\SettingsPackage\\SettingsPackageServiceProvider"
            ]
        }
    }
} 